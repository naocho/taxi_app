 <div class="menu">
  </div>
<%= form_with(url: search_taxis_path, local: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :title, placeholder: "タイトルで検索", class: "search-input" %>
  <%= form.submit "検索", class: "search-btn" %>
<% end %>
<div class="contents row">
  <% @taxis.each do |taxi| %>
    <%= render partial: "taxi-main", locals: { taxi: taxi } %>
  <% end %>
</div>