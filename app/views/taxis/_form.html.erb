    <%= form_with(model: taxi, local: true) do |form| %>
      <%= render 'error_messages', model: form.object %>
        <%= form.text_field :departure, placeholder: "出発地" %>
        <%= form.text_field :arrival, placeholder: "到着地" %>
        <%= form.file_field :image %>
        <%= form.text_field :title, placeholder: "タイトル" %>
        <%= form.text_area :text, placeholder: "感想", rows: "10" %>
        <%= form.submit "投稿" %>
    <% end %>